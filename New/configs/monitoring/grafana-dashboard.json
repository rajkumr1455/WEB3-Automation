{
  "dashboard": {
    "title": "Web3 Bounty Automation System",
    "tags": ["web3", "security", "automation"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Scans",
        "type": "stat",
        "targets": [
          {
            "expr": "orchestrator_active_scans",
            "legendFormat": "Active Scans"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Total Scans",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(orchestrator_scans_total[5m])",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "LLM Requests by Model",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(llm_router_requests_total[5m])",
            "legendFormat": "{{model_type}}/{{model_key}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "LLM Request Duration",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(llm_router_request_duration_seconds_bucket[5m])",
            "legendFormat": "{{model_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Agent Errors",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(orchestrator_agent_errors_total[5m])",
            "legendFormat": "{{agent}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Scan Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(orchestrator_scan_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(orchestrator_scan_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    }
  }
}
